	<parameter name="Consumer_Application_No" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
    cad.consumer_application_number AS applicatinno,
    con.consumer_name               AS consumername,
    con.address                     AS address,
    cad.short_description_of_work   AS sortdescription,
    dc.dc_name                      AS distributionname,
    sd.sub_division                 AS subdivision,
    div.division                    AS divisionname,
    cir.circle                      AS circlename,
    cd.company_name                 AS contractorname,
    cd.user_class                   AS classtype
FROM
    consumer_application_detail cad
    LEFT JOIN consumer                    con ON cad.consumer_id = con.consumer_id
    LEFT JOIN distribution_center         dc ON cad.dc_id = dc.dc_id
    LEFT JOIN sub_division                sd ON dc.subdiv_id = sd.subdiv_id
    LEFT JOIN division                    div ON sd.division_id = div.div_id
    LEFT JOIN circle                      cir ON div.circle_id = cir.circle_id
    LEFT JOIN contractor_detail           cd ON cad.contractor_id = cd.contractor_id
WHERE
    cad.consumer_application_number = $P{Consumer_Application_No} ]]>
	</queryString>
