   public String consumerApplicationStatusBack(ConsumerApplicationDetail consumerApplicationDetail) {
      ApplicationStatus appStatusDb = consumerApplicationDetail.getApplicationStatus();

      try {
         if (consumerApplicationDetail.getApplicationStatus().getApplicationStatusId() != 20L && consumerApplicationDetail.getApplicationStatus().getApplicationStatusId() != 21L && consumerApplicationDetail.getApplicationStatus().getApplicationStatusId() != 23L) {
            appStatusDb = this.applicationStatusService.findById(ApplicationStatusEnum.PENDING_FOR_REGISTRATION_FEES.getId());
         } else {
            if (consumerApplicationDetail.getSchemeType().getSchemeTypeName().equalsIgnoreCase("Deposit")) {
               appStatusDb = this.applicationStatusService.findById(ApplicationStatusEnum.DEMAND_PAYMENT_PENDING_BY_CONSUMER.getId());
            } else if (consumerApplicationDetail.getSchemeType().getSchemeTypeName().equalsIgnoreCase("Supervision") && consumerApplicationDetail.getNatureOfWorkType().getNatureOfWorkTypeId() != 5L) {
               appStatusDb = this.applicationStatusService.findById(ApplicationStatusEnum.DEMAND_PAYMENT_PENDING_BY_CONSUMER.getId());
            } else {
               appStatusDb = this.applicationStatusService.findById(ApplicationStatusEnum.DEMAND_PAYMENT_PENDING_BY_CONSUMER.getId());
            }

            Date currentDate = new Date();
            Calendar calendar = Calendar.getInstance();
            calendar.setTime(currentDate);
            calendar.add(5, 3);
            Date newDate = calendar.getTime();
            SimpleDateFormat formatter = new SimpleDateFormat("dd-MM-yyyy");
            String currentDateFormatted = formatter.format(currentDate);
            String newDateFormatted = formatter.format(newDate);
            System.out.println("Current Date: " + currentDateFormatted);
            System.out.println("New Date: " + newDateFormatted);
            if (consumerApplicationDetail.getSchemeType().getSchemeTypeName().equalsIgnoreCase("Supervision")) {
               consumerApplicationDetail.setPaymentDate(newDateFormatted);
            }
         }

         consumerApplicationDetail.setApplicationStatus(appStatusDb);
         this.consumerApplictionDetailRepository.save(consumerApplicationDetail);
         return "data save";
      } catch (Exception var9) {
         var9.printStackTrace();
         return null;
      }
   }

   @GetMapping({"/fees_genreted_recipt/{consumerApplicationNumber}/{slipGenretatedId}"})
   public void genretedRecipt(@PathVariable String consumerApplicationNumber, @PathVariable long slipGenretatedId, HttpServletResponse response1) {
      try {
         List<BillDeskPaymentResponse> payres1 = this.billPaymentResponseeeeeeeRepository.findByConsumerApplicationNo(consumerApplicationNumber);
         Iterator var6 = payres1.iterator();

         while(var6.hasNext()) {
            BillDeskPaymentResponse payres = (BillDeskPaymentResponse)var6.next();
            double Amount = Double.parseDouble(payres.getAmount());
            String redirectUrl;
            if (payres != null && slipGenretatedId == 1L && Amount < 1200.0D && payres.getAuth_status().equals("0300")) {
               redirectUrl = "/deposit_scheme/api/consumer/bill-desk/sendReciept?accountId=" + payres.getConsumerApplicationNo() + "&custName=" + payres.getConsumerName() + "&orderId=" + payres.getOrderid() + "&txnId=" + payres.getTranId() + "&txnAmount=" + payres.getAmount() + "&txnDate=" + payres.getTransactionDate() + "&status=Success&isFailed=" + Boolean.FALSE;
               response1.sendRedirect(redirectUrl);
            }

            if (payres != null && slipGenretatedId == 2L && Amount > 1200.0D && payres.getAuth_status().equals("0300")) {
               redirectUrl = "/deposit_scheme/api/consumer/bill-desk/sendReciept?accountId=" + payres.getConsumerApplicationNo() + "&custName=" + payres.getConsumerName() + "&orderId=" + payres.getOrderid() + "&txnId=" + payres.getTranId() + "&txnAmount=" + payres.getAmount() + "&txnDate=" + payres.getTransactionDate() + "&status=Success&isFailed=" + Boolean.FALSE;
               response1.sendRedirect(redirectUrl);
            }
         }
      } catch (Exception var11) {
         var11.printStackTrace();
      }

   }

